@model SporteStore___2nd_try.Models.ViewModels.ProductsListViewModel;
@foreach (var p in Model.Products ?? Enumerable.Empty<Product>())
{
    <partial name="ProductSummary" model="p"></partial>
}
<div class="btn-group pull-right m-1" page-model="@Model.PagingInfo" page-action="Index" page-classes-enabled="true"
    page-class="btn" page-class-normal="btn-outline-dark" page-class-selected="btn-primary"
    page-url-category="@Model.CurrentCategory!">
</div>
